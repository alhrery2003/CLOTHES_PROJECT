@model Employee


<table class="table table-striped mb-0">
    <thead style="background-color: #002d72; color:#f5f7fa;">
        <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Supervisor</th>
        <th>Address</th>
        <th>Sex</th>
        <th>Salary</th>
        <th>Birth Date</th>
        <th>Department</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td>@Model.SSN</td>
        <td>@(Model.Fname + " " + Model.Lname)</td>
        <td>@(ViewBag.SUPERVISOR.Fname+" "+ViewBag.SUPERVISOR.Lname)</td>
        <td>@Model.Address</td>
        <td>@Model.Sex</td>
        <td>@Model.Salary</td>
        <td>@Model.Bdate</td>
        <td>@ViewBag.DEPARTMENT.DName</td>
    </tr>
    </tbody>
</table>

<div class="text-center">
    <br />
    <br />
    <a asp-action="Index" asp-controller="Employee" class="btn btn-outline-info">Back To Employees</a>
</div>
